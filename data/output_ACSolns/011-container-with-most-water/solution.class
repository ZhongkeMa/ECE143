class Solution {
    public int maxArea(int[] height) {
       int high = height.length - 1;
       int low = 0;
        int area = 0;
        while(low<high){
            area = Math.max(area, Math.min(height[low], height[high]) * (high - low));
            if(height[low] > height[high])
                high--;
            else
                low++;
            
        }
        return area;
    }
}